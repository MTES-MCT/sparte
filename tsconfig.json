{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "typeRoots": ["./node_modules/@types", "./assets/types"],
        "baseUrl": "./assets/scripts",
        "paths": {
            "@components/*": ["components/*"],
            "@hooks/*": ["hooks/*"],
            "@store/*": ["store/*"],
        }
    },
}
